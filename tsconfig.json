{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "jsxFactory": "h",
    "jsxFragmentFactory": "FragmentDescriptor",
    "jsx": "react",
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
}
